    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'dev_utils'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=dev_utils"
                ],
                "filter": {
                    "name": "dev_utils",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'main_tester'",
            "cargo": {
                "args": [
                    "build",
                    "--example=main_tester",
                    "--package=dev_utils"
                ],
                "filter": {
                    "name": "main_tester",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in example 'main_tester'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--example=main_tester",
                    "--package=dev_utils"
                ],
                "filter": {
                    "name": "main_tester",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
